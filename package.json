{
  "name": "mkt4u-ai-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "jest --testPathPattern=performance",
    "test:unit": "jest --testPathPattern='\\.(test|spec)\\.(ts|tsx)$' --testPathIgnorePatterns='performance|integration|e2e'",
    "test:integration": "jest --testPathPattern=integration",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > types/database.ts",
    "db:migrate": "supabase db push",
    "db:seed": "node database/seed.js",
    "blockchain:build": "cd blockchain && cargo build --release",
    "blockchain:test": "cd blockchain && cargo test",
    "prepare": "husky install"
  },
